{
  "name": "cozy-bin",
  "version": "0.1.0",
  "scripts": {
    "clean": "rm -rf build/*",
    "tx": "tx pull --all || true",
    "lint": "npm-run-all --parallel 'lint:*'",
    "lint:js": "eslint '{src,test}/**/*.{js,jsx}'",
    "prettier": "prettier --write '{src,test}/**/*.{js,jsx}' && eslint --fix '{src,test}/**/*.{js,jsx}'",
    "lint:styles": "stylint src/styles --config ./.stylintrc",
    "build": "yarn run build:browser",
    "commons:build": "cozy-scripts build",
    "build:browser": "NODE_ENV=browser:production npm-run-all --serial clean lint:js tx commons:build",
    "watch": "yarn run watch:browser",
    "commons:watch": "cozy-scripts watch",
    "watch:browser": "NODE_ENV=browser:development npm-run-all --serial clean commons:watch",
    "commons:watch:standalone": "cozy-scripts standalone",
    "watch:browser:standalone": "NODE_ENV=browser:development yarn run commons:watch:standalone",
    "config:browser:production": "NODE_ENV=browser:production cozy-scripts --show-config",
    "config:browser:development": "NODE_ENV=browser:development cozy-scripts --show-config",
    "deploy": "git-directory-deploy --username y-lohse --email contact@yannick-lohse.fr --directory build/ --repo=https://$GITHUB_TOKEN@github.com/y-lohse/cozy-bin.git",
    "test": "npm-run-all --serial lint test:jest",
    "test:jest": "jest --verbose --coverage",
    "stack:docker": "docker run --rm -it -p 8080:8080 -p 5984:5984 -v \"$(pwd)/build\":/data/cozy-app/app cozy/cozy-app-dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/y-lohse/cozy-bin.git"
  },
  "author": "",
  "contributors": [],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/y-lohse/cozy-bin/issues"
  },
  "homepage": "https://github.com/y-lohse/cozy-bin#readme",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-jest": "22.2.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-cozy-app": "0.3.2",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.18.1",
    "eslint-config-cozy-app": "0.3.2",
    "git-directory-deploy": "1.5.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "22.3.0",
    "npm-run-all": "4.1.2",
    "prettier": "1.10.2",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-test-renderer": "16.2.0",
    "redux-logger": "3.0.6",
    "redux-mock-store": "1.5.1",
    "stylint": "1.5.9"
  },
  "dependencies": {
    "cozy-bar": "4.8.8",
    "cozy-client-js": "0.7.1",
    "cozy-scripts": "0.5.3",
    "cozy-ui": "7.5.0",
    "preact": "8.2.7",
    "preact-compat": "3.18.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-raven-middleware": "1.2.0",
    "redux-thunk": "2.2.0"
  }
}
